    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "photoswipe": "^5.4.3",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-geocode": "^1.0.0-alpha.1",
    "react-icons": "^5.2.0",
    "react-map-gl": "^7.1.7",
    "react-photoswipe-gallery": "^3.0.1",
    "react-share": "^5.1.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "18.3.1",
    "eslint": "^8",
