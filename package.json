{
  "name": "property-pulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.3",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.0",
    "react-spinners": "^0.13.8"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
